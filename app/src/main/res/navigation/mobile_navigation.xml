<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_post">

    <fragment
        android:id="@+id/navigation_find"
        android:name="com.petfinder.ui.find.FindPetFragment"
        android:label="@string/title_find"
        tools:layout="@layout/fragment_find" />

    <fragment
        android:id="@+id/navigation_post"
        android:name="com.petfinder.ui.post.PostPetFragment"
        android:label="@string/title_post"
        tools:layout="@layout/fragment_post" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.petfinder.ui.chat.SendFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_chat" />
</navigation>